{
  "name": "frontend",
  "version": "0.0.0",
  "devDependencies": {
    "@lucide/svelte": "^0.515.0",
    "@sveltejs/vite-plugin-svelte": "^6.0.0",
    "@tanstack/table-core": "^8.21.3",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@tsconfig/svelte": "^5.0.4",
    "clsx": "^2.1.1",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.11.0",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.2.3",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "svelte": "^5.35.5",
    "svelte-check": "^4.2.2",
    "tailwind-merge": "^3.3.1",
    "tailwind-variants": "^1.0.0",
    "tailwindcss": "^4.1.11",
    "tw-animate-css": "^1.3.6",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.38.0",
    "vite": "^7.0.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "src/**/*.{ts,svelte}": [
      "prettier --write",
      "bun run lint:staged"
    ]
  },
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint:staged": "eslint --ext .ts,.svelte --max-warnings=0 --fix",
    "lint": "eslint . --ext .ts,.svelte --max-warnings=0",
    "lint:fix": "bun run lint -- --fix",
    "lint:fix:test": "bun run lint:fix && bun run test",
    "prettier:fix": "prettier --write src/ *[.js,.ts]",
    "prettier:check": "prettier --check src/",
    "check": "svelte-check --tsconfig ./tsconfig.app.json && tsc -p tsconfig.node.json",
    "generate-client": "rm -rf src/client && npx @hey-api/openapi-ts -i http://localhost:8000/openapi.json -o src/client"
  },
  "type": "module",
  "dependencies": {
    "@tailwindcss/vite": "^4.1.11",
    "@tanstack/svelte-query": "^5.83.0",
    "globals": "^16.3.0"
  }
}
