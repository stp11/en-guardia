{
  "name": "frontend",
  "version": "0.0.0",
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^6.0.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@tsconfig/svelte": "^5.0.4",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "svelte": "^5.35.5",
    "svelte-check": "^4.2.2",
    "typescript": "~5.8.3",
    "vite": "^7.0.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,svelte}": [
      "prettier --write",
      "bun run lint:staged"
    ]
  },
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint:staged": "eslint --ext .ts,.tsx,.svelte --max-warnings=0 --fix",
    "lint": "eslint . --ext .ts,.tsx,.svelte --max-warnings=0",
    "lint:fix": "bun run lint -- --fix",
    "lint:fix:test": "bun run lint:fix && bun run test",
    "prettier:fix": "prettier --write src/",
    "prettier:check": "prettier --check src/",
    "check": "svelte-check --tsconfig ./tsconfig.app.json && tsc -p tsconfig.node.json",
    "generate-client": "rm -rf src/client && npx @hey-api/openapi-ts -i http://localhost:8000/openapi.json -o src/client",
    "prepare": "husky"
  },
  "type": "module"
}
